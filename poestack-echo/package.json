{
  "name": "poestack-sage",
  "description": "poestack v2",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf build/index.js dist",
    "build": "rimraf build/index.js && rollup -c --configPlugin typescript",
    "start": "npm run build && electron .",
    "make": "npm run build && rimraf dist && electron-builder"
  },
  "build": {
    "appId": "poestack.sage",
    "files": [
      "**/*",
      "./build/**/*"
    ],
    "extraMetadata": {
      "main": "main.js"
    },
    "mac": {
      "category": "tool"
    }
  },
  "dependencies": {
    "@react-rxjs/core": "^0.10.7",
    "@react-rxjs/utils": "^0.9.7",
    "electron-squirrel-startup": "^1.0.0",
    "poe-api": "file:../poe-api",
    "poestack-echo-common": "file:../poestack-echo-common",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rxjs": "^7.8.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "tailwindcss": "^2.2.19"
  }
}
