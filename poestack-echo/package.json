{
  "name": "your-electron-app",
  "description": "poestack v2",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf build dist",
    "build": "rimraf build && rollup -c --configPlugin typescript",
    "start": "npm run build && electron .",
    "make": "npm run build && rimraf dist && electron-builder"
  },
  "build": {
    "appId": "poestack.sage",
    "files": [
      "**/*",
      "./build/**/*"
    ],
    "extraMetadata": {
      "main": "main.js"
    },
    "mac": {
      "category": "tool"
    }
  },
  "dependencies": {
    "@react-rxjs/core": "^0.10.7",
    "@react-rxjs/utils": "^0.9.7",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "poestack-echo-common": "file:../poestack-echo-common",
    "poe-api": "file:../poe-api",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4",
    "rollup-plugin-peer-deps-external": "^2.2.3"
  }
}
