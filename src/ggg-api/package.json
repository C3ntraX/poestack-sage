{
  "name": "ggg-api",
  "version": "1.0.3",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist/"
  ],
  "exports": {
    "development": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "default": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig-esm.json && tsc -p tsconfig.json",
    "test": "npm run build && node dist/cjs/poe-api-test.js",
    "prepublishOnly": "npm run build",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --fix --resolve-plugins-relative-to ../eslint-config-sage"
  },
  "dependencies": {
    "axios": "^1.6.1"
  },
  "peerDependencies": {
    "@react-rxjs/core": "^0.10.7",
    "@react-rxjs/utils": "^0.9.7",
    "rxjs": "^7.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sage-common": "file:../sage-common",
    "tail": "^2.2.6",
    "ts-ratchet": "file:../ts-ratchet"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.15",
    "@types/tail": "^2.2.3",
    "typescript": "^5.2.2",
    "prettier": "^3.0.3",
    "eslint": "^8.53.0",
    "@typescript-eslint/parser": "^6.10.0"
  }
}
