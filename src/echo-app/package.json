{
  "name": "poestack-sage",
  "description": "poestack v2",
  "author": "PoEStack",
  "version": "1.0.0",
  "repository": "https://github.com/PoeStack/poestack-sage",
  "main": "./out/main/index.js",
  "scripts": {
    "clean": "rimraf build/index.js out && rimraf build/index.js dist",
    "dev": "npm run bump-dev-plugins && electron-vite dev",
    "start": "npm run bump-dev-plugins && electron-vite preview",
    "build": "npm run bump-dev-plugins && npm run typecheck && electron-vite build",
    "build:plugins": "cd ../../ && gradlew buildPlugins",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build build && electron-builder --mac --config",
    "build:linux": "npm run build build && electron-builder --linux --config",
    "bump-version": "node internal/bump-version.js",
    "bump-dev-plugins": "node internal/bump-dev-plugins.js",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --fix --resolve-plugins-relative-to ../sage-ts-tooling"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@electron/remote": "^2.0.12",
    "@libsql/client": "^0.4.0-pre.2",
    "body-parser": "^1.20.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.0",
    "echo-common": "file:../echo-common",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "libsql": "^0.2.0-pre.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-x64": "^4.3.0",
    "dmg-license": "^1.0.11"
  },
  "peerDependencies": {
    "@heroicons/react": "^2.0.18",
    "@react-rxjs/core": "^0.10.7",
    "@react-rxjs/utils": "^0.9.7",
    "axios": "^1.6.1",
    "ggg-api": "file:../ggg-api",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-fs-backend": "^2.3.0",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "rxjs": "^7.8.1",
    "sage-common": "file:../sage-common",
    "tail": "^2.2.6",
    "ts-ratchet": "file:../ts-ratchet"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.4",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lodash": "^4.17.21",
    "next-build-id": "^3.0.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-commonjs-externals": "^0.1.3",
    "vite-plugin-node-polyfills": "^0.16.0"
  }
}
